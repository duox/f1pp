<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<tests>
	<test id="test-cpp-newlines-cr" path="test://f1/pp/cpp/basic/newlines/cr" name="Tests CR (Mac) new line format">
		<stdout template="stdout.template.txt" />
		<inputs>
			<input test_file="test-cr.c" dest="test-cr.c.ii" /> 
		</inputs>
		<outputs>
			<output test_file="test-cr.c.ii" template="output-cr.template.txt" /> 
		</outputs>
	</test>
